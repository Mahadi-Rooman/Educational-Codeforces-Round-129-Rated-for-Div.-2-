    #include<bits/stdc++.h>
    #define ll long long
    #define loop_0(i,a,b) for(int i=a;i<b;i++)
    #define loop(i,a,b) for(int i=a;i<=b;i++)
    #define d_loop(i,a,b) for(int i=b-1;i>=a;i--)
    #define st set<ll>s;
    #define anika_a78 ios::sync_with_stdio(0);cin.tie(0);cout.tie(0);
    #define dec_map  map<ll,ll>mp
    #define pb push_back
    #define fst first
    #define snd second
    #define dec_pair pair<ll,ll>pa
    #define vec vector<ll>vc
    #define dec_queue queue<ll>qu
    #define mop map<ll,vector<ll>>mp
    #define Max(a,b) max(a,b)
    #define Min(a,b) min(a,b)
    #define dc 1e9
    # define all(x) x.begin(),x.end()
    #define MAX(a, b) (((a) > (b)) ? (a) : (b))
    #define MIN(a, b) (((a) > (b)) ? (b) : (a))
     
    using namespace std;
    int main()
    {
       anika_a78;
       int tc;
       cin>>tc;
       while(tc--)
       {
       	   ll n;
       	   cin>>n;
       	   vector<int>a(n);
       	   loop_0(i,0,n)
       	     cin>>a[i];
       	   ll m;
       	   cin>>m;
       	   vector<int>b(m);
       	   ll x=0;
       	   loop_0(i,0,m)
       	   {
       	   	   cin>>b[i];
       	   	   x+=b[i];
       	   	   x%=n;
       	   }
       	 
       	   cout<<a[x]<<endl;
       }
    }
     
